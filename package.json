{
  "name": "umbrel-middleware",
  "version": "0.5.0",
  "description": "Middleware for Umbrel Node",
  "author": "Umbrel",
  "scripts": {
    "lint": "eslint",
    "start": "node ./bin/www",
    "install:ui": "cd ui && yarn",
    "build:ui": "cd ui && yarn build",
    "serve:ui": "cd ui && yarn serve",
    "test": "mocha --file test.setup 'test/**/*.js'",
    "coverage": "nyc --all mocha --file test.setup 'test/**/*.js'",
    "postcoverage": "codecov"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "big.js": "^5.2.2",
    "bitcoind-rpc": "^0.7.2",
    "body-parser": "^1.18.2",
    "camelize-keys": "^1.0.0",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.5",
    "debug": "^2.6.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "lodash.merge": "^4.6.2",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.0",
    "request-promise": "^4.2.2",
    "uuid": "^3.3.2",
    "winston": "^3.0.0-rc5",
    "winston-daily-rotate-file": "^3.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "codecov": "^3.7.1",
    "eslint": "^5.3.0",
    "mocha": "^5.2.0",
    "nyc": "13.0.1",
    "proxyquire": "^2.0.1",
    "sinon": "^6.1.4"
  },
  "nyc": {
    "exclude": [
      "test",
      "test.setup.js"
    ],
    "sourceMap": false,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": "false"
  }
}
